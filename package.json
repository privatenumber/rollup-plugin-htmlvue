{
	"name": "rollup-plugin-htmlvue",
	"version": "0.0.0-semantic-release",
	"description": "Rollup plugin for transforming HTML/XML to Vue SFC",
	"keywords": [
		"rollup",
		"rollup plugin",
		"vue",
		"html",
		"xml",
		"svg"
	],
	"license": "MIT",
	"repository": "privatenumber/rollup-plugin-htmlvue",
	"funding": "https://github.com/privatenumber/rollup-plugin-htmlvue?sponsor=1",
	"author": {
		"name": "Hiroki Osame",
		"email": "hiroki.osame@gmail.com"
	},
	"files": [
		"dist"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.cts",
	"exports": {
		"require": {
			"types": "./dist/index.d.cts",
			"default": "./dist/index.cjs"
		},
		"import": {
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		}
	},
	"imports": {
		"#rollup-plugin-htmlvue": {
			"types": "./src/index.ts",
			"development": "./src/index.ts",
			"default": "./dist/index.mjs"
		}
	},
	"scripts": {
		"lint": "eslint .",
		"build": "pkgroll",
		"typecheck": "tsc",
		"test": "tsx tests",
		"dev": "tsx watch --conditions=development tests"
	},
	"peerDependencies": {
		"rollup": "^2.0.0"
	},
	"dependencies": {
		"@rollup/pluginutils": "^5.1.0",
		"cheerio": "1.0.0-rc.12"
	},
	"devDependencies": {
		"@happy-dom/global-registrator": "^13.3.5",
		"@pvtnbr/eslint-config": "^0.38.0",
		"@types/cheerio": "^0.22.35",
		"@types/node": "^20.11.10",
		"@vitejs/plugin-vue": "^5.0.3",
		"@vue/test-utils": "^2.4.3",
		"eslint": "^8.56.0",
		"fs-fixture": "^1.2.0",
		"manten": "^1.2.0",
		"pkgroll": "^2.0.1",
		"rollup": "^2.79.1",
		"tsx": "^4.7.0",
		"typescript": "^5.3.3",
		"vue": "^3.4.15"
	},
	"eslintConfig": {
		"extends": "@pvtnbr"
	}
}
